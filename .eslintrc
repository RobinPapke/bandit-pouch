{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "plugins": ["react", "import", "jest"],
  "rules": {
    "max-len": ["error", { "code": 120 }],
    "react/jsx-filename-extension": "off",
    "react/forbid-prop-types": "off"
  }
}
